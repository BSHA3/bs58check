{
  "name": "bs58check",
  "version": "1.0.3",
  "description": "A straightforward implementation of base58-check encoding",
  "keywords": [
    "base58",
    "base58check",
    "bitcoin",
    "bs58",
    "decode",
    "decoding",
    "encode",
    "encoding",
    "litecoin"
  ],
  "main": "./src/bs58check.js",
  "repository": {
    "url": "http://github.com/dcousens/bs58check",
    "type": "git"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.2",
    "mocha": "^1.21.4"
  },
  "testling": {
    "browsers": [
      "android-browser/4.2..latest",
      "chrome/20..latest",
      "firefox/21..latest",
      "ipad/6..latest",
      "iphone/6..latest",
      "opera/15..latest",
      "safari/latest"
    ],
    "harness": "mocha-bdd",
    "files": "test/*.js"
  },
  "scripts": {
    "coverage": "istanbul cover _mocha -- test/*.js",
    "coveralls": "npm run coverage && coveralls < coverage/lcov.info",
    "test": "istanbul test mocha -- --reporter list test/*.js"
  },
  "dependencies": {
    "bs58": "^2.0.0"
  }
}
